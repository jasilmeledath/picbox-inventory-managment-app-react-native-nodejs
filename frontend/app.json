{
  "expo": {
    "name": "PicBox Inventory",
    "slug": "picbox-inventory",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/logos/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.echosounds.picbox"
    },
    "android": {
      "package": "com.echosounds.picbox",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "favicon": "./assets/logos/app-icon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3f4adbe2-3fe2-4570-a8c9-cb8503cbed0e"
      }
    }
  }
}
